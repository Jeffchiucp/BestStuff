{
  "name": "Best-Stuff",
  "version": "0.0.1",
  "description": "Food review app for Make School's Web 3 - Advanced Web Patterns.",
  "main": "src/main.js",
  "homepage": "https://beststuff.herokuapp.com",
  "author": "Tassos Lambrou, Briant Elliot, Alirie Gray, Rohan Mishra",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tlambrou/iHangry"
  },
  "keywords": [
    "react",
    "express",
    "postcss",
    "nodejs",
    "sequelize"
  ],
  "scripts": {
    "start": "better-npm-run start",
    "build:start": "better-npm-run build:start",
    "build": "rimraf dist && better-npm-run build",
    "postinstall": "npm run build"
  },
  "standard": {
    "globals": [
      "__DEV__"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-no-bind": 2,
      "react/no-did-update-set-state": 2,
      "react/no-unknown-property": 2,
      "react/prop-types": 2
    }
  },
  "betterScripts": {
    "start": {
      "command": "node server.development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:start": {
      "command": "node server.production",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^1.16.5",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "autoprefixer": "^6.0.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "better-npm-run": "0.0.7",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.19.0",
    "css-modules-require-hook": "^4.2.2",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "google-maps-react": "^1.1.0",
    "html-webpack-plugin": "^2.9.0",
    "lodash": "^4.5.0",
    "node-jsx": "^0.13.3",
    "node-sass": "^3.4.2",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "postcss": "^6.0.13",
    "postcss-loader": "^0.6.0",
    "postcss-modules-resolve-imports": "^1.3.0",
    "postcss-nested": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^0.14.9",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.9",
    "react-router": "^2.8.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sequelize": "^4.20.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  }
}
